{
	"compileOnSave": false,
	"compilerOptions": {
		"moduleResolution": "Node",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"outDir": "./dist",
		"declaration": false,
		"target": "ESNext",
		"module": "ESNext"
	},
	"include": ["."],
	"exclude": ["node_modules", ".husky", "dist", ""]
}
